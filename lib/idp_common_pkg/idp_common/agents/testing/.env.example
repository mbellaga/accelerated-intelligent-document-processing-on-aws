# Example environment variables for testing the analytics agent
# Copy this file to .env and update with your actual values

# Required: Your Athena database name
ATHENA_DATABASE=your_database_name

# Required: S3 location for Athena query results
ATHENA_OUTPUT_LOCATION=s3://your-bucket/athena-results/

# Optional: AWS region (defaults to us-east-1)
AWS_REGION=us-east-1

# Optional: Application logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
# Controls logging for the idp_common package and other application code
LOG_LEVEL=INFO

# Optional: Strands framework logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
# Controls logging specifically for the Strands framework
# Set to DEBUG to see detailed Strands agent interactions and LLM prompts
STRANDS_LOG_LEVEL=INFO